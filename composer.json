{
    "name": "flarum-lang/thai",
    "type": "flarum-extension",
    "description": "Thai language pack for Flarum.",
    "homepage": "https://github.com/flarum-lang/thai",
    "keywords": [
        "locale",
        "flarum",
        "thai-locale"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "kon3ko",
            "email": "akoneko47@gmail.com",
            "homepage": "https://nekocloud.io/"
        }
    ],
    "support": {
        "issues": "https://github.com/flarum-lang/thai/issues",
        "source": "https://github.com/flarum-lang/thai"
